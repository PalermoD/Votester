<%= render 'static_pages/loginModel' %>
<%= render 'static_pages/signupModel' %>
<% if logged_in? %>
<div class="row">
	
		<section class="user_info">
			<%= render 'shared/user_info' %>
		</section>
		<section class="micropost_form">
			<%= render 'shared/post_form' %>
		</section>
		<section>
            <%= render 'shared/feed' %>
        </section>

</div>
<% else %>
   <section>
       <%= render 'shared/feed' %>
   </section>
     
<% end %>
